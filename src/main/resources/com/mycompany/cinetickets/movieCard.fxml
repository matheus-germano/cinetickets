<AnchorPane fx:id="anchorId" prefHeight="150.0" prefWidth="300.0" style="-fx-background-color: fff; -fx-border-radius: 5;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.cinetickets.Components.MovieCard">
   <children>
      <ImageView fx:id="moviePoster" fitHeight="150.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="movieTitle" layoutX="85.0" layoutY="14.0" prefHeight="17.0" prefWidth="119.0" text="Nome" wrapText="true">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="movieDuration" layoutX="239.0" layoutY="14.0" prefHeight="17.0" prefWidth="47.0" text="00:00:00" />
      <GridPane fx:id="sessionsGrid" layoutX="80.0" layoutY="51.0" prefHeight="0.0" prefWidth="0.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
      </GridPane>
   </children>
</AnchorPane>
